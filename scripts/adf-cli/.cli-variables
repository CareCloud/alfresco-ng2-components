#!/usr/bin/env bash

SCRIPTSDIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )/.." && pwd )"
COMPONENTSDIR="$SCRIPTSDIR/../ng2-components"
DEMOSHELLDIR="$SCRIPTSDIR/../demo-shell-ng2"

SCRIPTS="scripts"
COMPONENTS_DIRNAME="components"
COMPONENTS=(`ls $COMPONENTSDIR | grep ng2 | sed 's/^ng2-activiti-//' | sed 's/^ng2-alfresco-//' | sed -e ':a' -e 'N' -e '$!ba' -e 's/\n/ /g'`)
DEMOSHELL="demo"